{
  "name": "jigsaw-puzzle",
  "version": "0.1.0",
  "homepage": "http://stefanomasini.github.io/jigsaw-puzzle",
  "private": true,
  "dependencies": {
    "bootstrap": "4",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "reactstrap": "^6.5.0",
    "seedrandom": "^2.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write --print-width 160 --tab-width 4 --trailing-comma es5 --single-quote --jsx-bracket-same-line",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
